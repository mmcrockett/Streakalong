      <div class="navbar navbar-default" id="items">
        <ul class="nav nav-tabs">
          <% @user.categorized_items.each do |category| %>
            <% category.each_key do |category_name| %>
              <li class="<%= if (true == @user.preferredTab?(category_name)); 'active'; end %>"><a href="#<%=category_name%>" data-toggle="tab"><%=category_name%></a></li>
            <% end %>
          <% end %>
        </ul>
        <div class="tab-content">
        <% @user.categorized_items.each do |category| %>
          <% category.each_pair do |category_name,list| %>
            <div class="tab-pane <%= if (true == @user.preferredTab?(category_name)); 'active'; end %>" id="<%=category_name%>">
            <% list.each do |item_type| %>
              <span ng-dblclick="process_amount('+1',<%= Item.id(item_type) %>, selected_date())">
              <%= render :partial => "common/item_image", :locals => {:category => category_name, :type => item_type, :id => Item.id(item_type)} %>
              </span>
            <% end %>
            </div>
          <% end %>
        <% end %>
        </div>
      </div>
